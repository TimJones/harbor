#!/bin/sh

OS_SERVICE=$1

source /etc/os-container.env
source /opt/harbor/marina/load-config-helper.sh
export OS_DOMAIN=${NETWORK_DEFAULT_OS_DOMAIN}
source /opt/harbor/service-hosts.sh
source /opt/harbor/harbor-common.sh

check_required_vars OS_SERVICE

load_node_config
load_image_config ${OS_SERVICE}
load_network_config
load_auth_config ${OS_SERVICE}
